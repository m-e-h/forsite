{
  "name": "m-e-h/forsite",
  "description": "Starter theme for WordPress.",
  "type": "wordpress-theme",
  "license": "GPL-3.0-or-later",
  "require": {
    "justintadlock/hybrid-carbon": "^1.0",
    "johnbillion/extended-cpts": "^4.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5.5",
    "automattic/phpcs-neutron-standard": "^1.6.0",
    "sirbrillig/phpcs-import-detection": "^1.2.2",
    "sirbrillig/phpcs-variable-analysis": "^2.8.2",
    "wp-coding-standards/wpcs": "^2.3.0",
    "phpcsstandards/phpcsutils": "1.0.0-alpha2",
    "wp-cli/i18n-command": "^2.2.3"
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "autoload": {
    "psr-4": {
      "Forsite\\": "inc"
    }
  },
  "scripts": {
    "phpcbf-dev": "\"vendor/bin/phpcbf\"",
    "run-phpcbf": [
      "@phpcbf-dev"
    ],
    "phpcs-dev": "\"vendor/bin/phpcs\"",
    "run-phpcs": [
      "@phpcs-dev"
    ],
    "make-pot": "wp i18n make-pot . languages/_s.pot"
  }
}
